{
  "manifest_version": 2,
  "name": "Medata",
  "homepage_url": "http://localhost:8080/",
  "description": "Medata Plugin for dl.acm.org",
  "version": "1.0",
  "author": "Jan Effenberge, Jan Bode, Max Heydenmann, Kevin Kraus",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html", 
    "default_title": "Medata",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
